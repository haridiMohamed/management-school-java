<
script >
    $(document).ready(function() {
        $('#aioConceptName').change(function() {
            var color = $('#aioConceptName').find(":selected").text();
            var size = $('#selectsize').find(":selected").text();
            var refProduct = $('#refProduct').val();
            $.ajax({
                type: 'get',
                url: '/GetStock/' + color + "/" + size + "/" + refProduct,
                success: function(result) {
                    $("#qtyiteam").val(result);
                    $('#result').text(result);
                }
            });
        });
    }); <
/script>  <
script >
    $(document).ready(function() {
        $('#selectsize').change(function() {
            var color = $('#aioConceptName').find(":selected").text();
            var size = $('#selectsize').find(":selected").text();
            var refProduct = $('#refProduct').val();
            $.ajax({
                type: 'get',
                url: '/GetStock/' + color + "/" + size + "/" + refProduct,
                success: function(result) {
                    $("#qtyiteam").val(result);
                    $('#result').text(result);
                }
            });
        });
    }); <
/script> 

