script type = "text/javascript" >

    $(function() {
        var name = $('.name').val();

        $("#aioConceptName").on("change", function() {
            $("#color").text($("#aioConceptName").val());
        }).trigger("change");

        $("#selectsize").on("change", function() {
            $("#size").text($("#selectsize").val());
        }).trigger("change");

        $("#qte").on("change", function() {
            $("#qty").text($("#qte").val());
        }).trigger("change");
    });
